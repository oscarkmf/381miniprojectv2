<html>
	<body>
	<head>
		<title>Result of restaurant after searching</title>
		<h1>Result of restaurant after searching</h1>
		<p>Showing <%=rest.length%> documents</p>
	</head>
	<% if (rest.length != 0) { %>	
	<table>
		<tr>
			<th>
				Name
			</th>
		</tr>
		<% for (var i = 0; i < rest.length; i++) { %>
			<tr>
				<td>
					<a href=/display?_id=<%= rest[i]._id %>><p><%= rest[i].name %></p>
				</td>	
			</tr>
		<% } %>
	</table>	
	<% } else { %>
		<p>
			There is no record!
		</p>
	<% } %>
	</br>
	<form action="/main">
		<input type="submit" value="Back to main page">
	</form>
	</br>
	<form action="/logout">
		<input type="submit" value="Logout">
	</form>
	</body>
</html>

